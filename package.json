{
  "name": "tinycode",
  "version": "1.0.0",
  "description": "A tiny code assistant CLI tool powered by AI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "tinycode": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "postbuild": "chmod +x dist/index.js",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["cli", "code-assistant", "ai", "development", "tools"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.1",
    "kleur": "^4.1.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
